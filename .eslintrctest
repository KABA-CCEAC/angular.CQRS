{
   "rules": {
      "browser": true,
      "camelcase": 1,
      "no-global-strict": 2,
      "strict": 0,
      "quotes": [2, "single"],
      "if-curly-formatting": 0,
      "angular-module-declaration": 1,
      "angular-component-named-functions": 1 ,
      "no-console":0,
      "no-undef":1
   },
   "globals": {
      "angular": false,
      "it": false,
      "describe": false,
      "inject":false,
      "expect":false,
      "beforeEach": false,
      "dump": false,
      "window":false,
      "console":false
   }
}